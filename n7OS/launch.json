{
    "type": "gdb",
    "request": "attach",
    "name": "Attach to gdbserver ",
    "executable": "${ workspaceRoot}/kernel.bin",
    "target": ":1234",
    "remote": true,
    "cwd": "${ workspaceRoot }",
    "debugger_args": ["-i=mi "],
    "valuesFormatting": "parseText",
    "autorun": [
        "dir kernel",
        "dir boot",
        "dir bin",
        "dir lib"
    ]
}

    